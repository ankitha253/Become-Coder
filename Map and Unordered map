#include<bits/stdc++.h>
using namespace std;
int main()
{
	map<int,int>mpp;
	int arr[10]={7,2,1,3,4,3,1,6,7,9};
	for(int i=0;i<10;i++){
		mpp[arr[i]]++;//{1:3,2:1,3:2,4:1,6:1,7:1,9:1}
	}
	for (auto it:mpp){
		cout<<it.first<<" "<<it.second<<"\n";
	}
	cout<<"\n";
	unordered_map<int,int>mp;
	int arr1[10]={7,3,1,3,4,4,1,6,7,9};
	for(int i=0;i<10;i++){
		mp[arr[i]]++;//{1:3,2:1,3:2,4:1,6:1,7:1,9:1}
	}
	for (auto it1:mp){
		cout<<it1.first<<" "<<it1.second<<"\n";
	}
}

//output:
1 2
2 1
3 2
4 1
6 1
7 2
9 1

9 1
6 1
4 1
7 2
2 1
1 2
3 2
